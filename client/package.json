{
  "name": "react-webpack-rails-tutorial",
  "version": "0.0.1",
  "engines": {
    "node": "5.10.0",
    "npm": "3.5.0"
  },
  "scripts": {
    "babel": "babel --out-dir babel-output app/libs/components",
    "babel-test": "babel --out-dir compiled_tests tests",
    "test": "mocha --compilers js:babel-core/register --require ./tests/helpers/test_helper.js \"tests/*@(.js|.jsx)\"",
    "build:test": "webpack --config webpack.config.js",
    "build:production": "NODE_ENV=production webpack --config webpack.config.js",
    "build:development": "webpack -w --config webpack.config.js"
  },
  "cacheDirectories": [
    "node_modules",
    "client/node_modules"
  ],
  "dependencies": {
    "axios": "^0.14.0",
    "babel": "^6.5.2",
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.4",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-runtime": "^6.6.1",
    "babel-tape-runner": "^2.0.1",
    "es5-shim": "^4.5.7",
    "expose-loader": "^0.7.1",
    "immutable": "^3.7.6",
    "imports-loader": "^0.6.5",
    "mirror-creator": "1.1.0",
    "react": "^0.14.8 || ^15.0.0",
    "react-addons-pure-render-mixin": "^15.3.1",
    "react-dom": "^0.14.8 || ^15.0.0",
    "react-on-rails": "6.3.2",
    "react-redux": "^4.4.5",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.7",
    "redux": "^3.5.2",
    "redux-thunk": "^2.1.0",
    "vanilla-ujs": "^1.3.0",
    "webpack": "^1.12.14"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "chai-immutable": "^1.6.0",
    "jsdom": "^9.4.5",
    "mocha": "^3.0.2",
    "react-addons-test-utils": "^15.3.1",
    "tap-spec": "^4.1.1",
    "tape": "^4.5.1"
  }
}
